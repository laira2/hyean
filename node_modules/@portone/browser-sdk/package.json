{
  "name": "@portone/browser-sdk",
  "type": "module",
  "version": "0.0.7",
  "description": "PortOne SDK for browser",
  "keywords": [
    "PortOne",
    "payment",
    "포트원",
    "결제 연동"
  ],
  "author": {
    "name": "portone-dx",
    "email": "support@portone.io"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/v2.cjs",
  "module": "./dist/v2.js",
  "types": "./dist/v2/index.d.ts",
  "typesVersions": {
    "*": {
      "v2": [
        "dist/v2"
      ]
    }
  },
  "exports": {
    "./v2": {
      "types": "./dist/v2/index.d.ts",
      "require": "./dist/v2.cjs",
      "import": "./dist/v2.js"
    }
  },
  "config": {
    "dirBuild": "./dist",
    "dirSrc": "./src"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "rollup": "^3.26.3",
    "tsconfig-replace-paths": "^0.0.14",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsconfig-replace-paths -s src"
  }
}